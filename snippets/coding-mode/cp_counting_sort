# -*- mode: snippet -*-
# name: cp_counting_sort
# key: cp_counting_sort
# --
#include <bits/stdc++.h>

using namespace std;

void solve()
{
  int a[] = {2, 5, 2, 2, 3, 3};
  int n = 6;
  int f[4];   // as 5 - 2 + 1 = 4
  int L = 2, R = 5;
  int b[n];

  // initialize
  for (int i = 0; i < 4; i++) {
    f[i] = 0;
  }

  for (int i = 0; i < n; i++) {
    f[a[i] - L]++;
  }

  // commulative frequency
  for (int i = 1; i < 4; i++) {
    f[i] = f[i] + f[i-1];
  }

  for (int i = n-1; i >= 0; i--) {
    int index = f[a[i]-L]-1;
    b[index] = a[i];
    f[a[i]-L]--;
  }

  for (int i = 0; i < n; i++) {
    printf("%d ", b[i]);
  }
  printf("\n");
}

int main() {
  ios::sync_with_stdio(0);
  cin.tie(0);
  solve();$0
  return 0;
}